
<p> Create your portfolio</p>
<%= form_for :portfolio do |p| %>
  <%= p.label :location %></br>
  <%= p.text_field :location %></br>

  <%= p.label :budget %></br>
  <%= p.text_field :budget %></br>

  <%= p.label :description %></br>
  <%= p.text_area :description %></br>

  <%= p.label :githuburl, "Github Url"%></br>
  <%= p.text_field :githuburl %></br>

  <%= p.submit %>
<% end %>  
